{
    "manifest_version": 2,
    "name": "Take ScreenShot",
    "version": "1.0",
    "description": "A simple 'Hello World' Chrome extension to take screenshot",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
    "permissions": [
      "https://*/*",
      "http://*/*",
      "tabs"
    ],
    "browser_action": {
      "default_title": "Inject!"
    }
  }
  